{
  "name": "tech-stack-showcase",
  "version": "1.0.0",
  "description": "A tech stack showcase website with TinaCMS integration for Netlify",
  "main": "build.js",
  "scripts": {
    "build": "node build.js && tinacms build",
    "dev": "tinacms dev -c \"node dev-server.js\"",
    "dev:tina": "tinacms dev",
    "init-content": "node scripts/init-content.js",
    "fetch-icons": "node scripts/fetch-icons.js",
    "lint": "npx @biomejs/biome check .",
    "lint:fix": "npx @biomejs/biome check . --write"
  },
  "keywords": [
    "tech-stack",
    "portfolio",
    "tinacms",
    "static-site",
    "netlify"
  ],
  "author": "Moana",
  "license": "MIT",
  "dependencies": {
    "@tinacms/cli": "^1.11.0",
    "chokidar": "^3.6.0",
    "express": "^4.22.1",
    "fs-extra": "^11.3.3",
    "handlebars": "^4.7.8",
    "markdown-it": "^14.1.1",
    "tinacms": "^2.9.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4"
  }
}
